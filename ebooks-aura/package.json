{
  "name": "ebooks-aura",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:dev": "cross-env NEXT_PUBLIC_API_URL=http://localhost:5000/api next build",
    "build:prod": "cross-env NEXT_PUBLIC_API_URL=https://ebookaura.onrender.com/api next build",
    "build:static": "next build && next export -o out",
    "build:static:dev": "cross-env STATIC_EXPORT=true NEXT_PUBLIC_API_URL=http://localhost:5000/api next build",
    "build:static:prod": "cross-env NODE_ENV=production next build && next export -o out",
    "start": "next start",
    "serve": "npx serve out",
    "lint": "next lint",
    "clean": "rimraf .next out",
    "build:production": "node build-production.js",
    "generate:sitemap": "node scripts/generate-sitemap.js",
    "generate:simple-sitemap": "node scripts/generate-simple-sitemap.js",
    "generate:pdf-metadata": "node scripts/generate-pdf-metadata.js",
    "generate:pdf-meta-txt": "node scripts/generate-pdf-meta-txt.js",
    "seo:all": "npm run generate:sitemap && npm run generate:pdf-metadata && npm run generate:pdf-meta-txt"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@react-pdf-viewer/core": "^3.12.0",
    "@react-pdf-viewer/default-layout": "^3.12.0",
    "date-fns": "^4.1.0",
    "next": "15.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-pdf": "^9.2.1",
    "react-pdf-viewer": "^0.1.0",
    "react-toastify": "^11.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.3.0",
    "raw-loader": "^4.0.2",
    "rimraf": "^5.0.10",
    "serve": "^14.2.1"
  }
}
